{
  "family": "strapi-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::301782007642:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::301782007642:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "strapi",
      "image": "301782007642.dkr.ecr.ap-southeast-1.amazonaws.com/strapi-app-judson:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 1337,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
